<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkVBox" id="Stellarium_VBox">
    <property name="visible">True</property>
    <child>
      <object class="GtkLabel" id="Stellarium_Label">
        <property name="visible">True</property>
        <property name="label" translatable="yes">Stellarium</property>
      </object>
      <packing>
        <property name="expand">False</property>
        <property name="position">0</property>
      </packing>
    </child>
    <child>
      <object class="GtkDrawingArea" id="Stellarium_DrawingArea">
        <property name="visible">True</property>
        <signal name="expose_event" handler="GLDraw"/>
        <signal name="realize" handler="GLInit"/>
        <signal name="configure_event" handler="GLResize"/>
      </object>
      <packing>
        <property name="position">1</property>
      </packing>
    </child>
  </object>
</interface>
